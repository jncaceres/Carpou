<%= react_component("devise/auth/sign-inForm", {user_session_path: user_session_path(resource), forgot_password_path: new_user_password_path}) %>
<% if resource.errors.any? %>
  </div>
      <% resource.errors.full_messages.each do |message| %>
        <%= react_component("devise/auth/error", {error: message}) %>
      <% end %>
  </div>
<% end %>


